    "version": "1.0.0.0",
    "description": "KIWI - Allergy Filter",
    "manifest_version": 3,
    "permissions": ["storage", "scripting", "activeTab"],
    "host_permissions": ["<all_urls>", "https://api.openai.com/*", "https://www.walmart.ca/*"],
    "action": {
        "default_popup": "index.html",
        "default_title": "Kiwi"
